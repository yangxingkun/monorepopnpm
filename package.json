{
  "name": "serescnn",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "pnpm -r --filter=./packages/* run build",
    "watch": "pnpm -r --filter=./packages/* run watch",
    "test": "vitest run",
    "test:report": "vitest --ui --coverage",
    "docs:dev": "pnpm run -C ./docs docs:dev",
    "docs:build": "pnpm run -C ./docs docs:build",
    "docs:preview": "pnpm run -C ./docs docs:preview",
    "prettier": "prettier --check ./",
    "prettier:fix": "prettier --write ./",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint --fix",
    "start:app1": "pnpm run start --filter @packages/app1",
    "eject:app1": "pnpm run eject --filter @packages/app1",
    "eject": "pnpm run -C packages/app1 eject && pnpm run -C packages/app2 start"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@serescnn/pkga": "workspace:^0.2.0",
    "@serescnn/pkgb": "workspace:^1.2.0",
    "@serescnn/pkgc": "workspace:^",
    "@types/node": "^20.11.26",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitest/coverage-istanbul": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^3.2.5",
    "prettier-config-standard": "^7.0.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.3.1"
  }
}
